} do
	if type(lang_etc) ~= "table" then
		lang_etc = {lang_etc}
	end
	local lang, desc = unpack(lang_etc)
	desc = desc or ("the [[:Category:%s language|%s language]]"):format(lang, lang)
	labels[lang] = {
		type = "topic",
		description = "=" .. desc,
		parents = {"languages"},
	}
